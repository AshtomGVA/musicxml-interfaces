{
    "name": "ripieno-musicxml-jsonify",
    "targetType": "staticLibrary",
    "sourcePaths": ["src"],
    "importPaths": [
        "src",
        "../src",
        ".."
    ],
    "stringImportPaths": [
        "../resources/dtd",
        "../resources/tests"
    ],
    "dflags-ldc": ["-relocation-model=pic"],
    "dflags-dmd": ["-fPIC"],
    "buildTypes": {
        "debug": {
            "buildOptions": ["debugMode"]
        },
        "unittest": {
            "buildOptions": ["unittests", "debugMode"]
        },
        "release": {
            "buildOptions": ["releaseMode", "optimize", "inline"]
        }
    }
}
